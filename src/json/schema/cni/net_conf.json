{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "cniVersion": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "type": {
            "type": "string"
        },
        "bridge": {
            "type": "string"
        },
        "isGateway": {
            "type": "boolean"
        },
        "ipMasq": {
            "type": "boolean"
        },
        "hairpinMode": {
            "type":"boolean"
        },
        "ipam": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string"
                },
                "routes": {
                    "type": "array",
                    "items": {
                        "$ref": "network/route.json"
                    }
                },
                "ranges": {
                    "type": "array",
                    "items": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "subnet": {
                                    "type": "string"
                                },
                                "rangeStart": {
                                    "type": "string"
                                },
                                "rangeEnd": {
                                    "type": "string"
                                },
                                "gateway": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "dns": {
            "$ref": "network/dns.json"
        },
        "args": {
            "$ref": "net_args.json"
        },
        "prevResult": {
            "$ref": "result_curr.json"
        },
        "runtimeConfig": {
            "type": "object",
            "properties": {
                "portMappings": {
                    "type": "array",
                    "items": {
                        "$ref": "inner_port_mapping.json"
                    }
                },
                "ipRanges": {
                    "type": "array",
                    "items": {
                        "type": "array",
                        "items": {
                            "$ref": "ip_ranges.json"
                        }
                    }
                },
                "bandwidth": {
                    "$ref": "bandwidth_entry.json"
                },
                "dns": {
                    "type": "object",
                    "properties": {
                        "searches": {
                            "$ref": "../defs.json#/definitions/ArrayOfStrings"
                        },
                        "servers": {
                            "$ref": "../defs.json#/definitions/ArrayOfStrings"
                        }
                    }
                },
                "ips": {
                    "$ref": "../defs.json#/definitions/ArrayOfStrings"
                },
                "mac": {
                    "type": "string"
                },
                "infinibandGUID": {
                    "type": "string"
                },
                "deviceID": {
                    "type": "string"
                }
            }
        },
        "capabilities": {
            "$ref": "../defs.json#/definitions/mapStringBool"
        }
    }
}
